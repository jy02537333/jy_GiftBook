    <style name="bg_line">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:background">?android:attr/listDivider</item>
    </style>

    <style name="f_sidekicker_group_item_lay_style">
        <item name="android:background">#ffffff</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="f_sidekicker_group_item_icon_style">
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:layout_marginTop">7.5dp</item>
        <item name="android:layout_width">30dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:src">@mipmap/f_sidekicker_group_item</item>
    </style>
    <style name="f_sidekicker_group_item_title_style">
        <item name="android:layout_weight">5</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:layout_marginLeft">10dp</item>
    </style>
    <style name="f_sidekicker_group_item_num_style">
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor"> #2D95EF</item>
    </style>
    <style name="f_sidekicker_group_item_more_style">
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:layout_marginTop">7.5dp</item>
        <item name="android:layout_width">30dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:src">@mipmap/aboutmore</item>
    </style>
</resources>
